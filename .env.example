# Supabase Public (Client-side)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# For Next.js compatibility (optional)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# E2E Testing - Service Role (bypasses RLS)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# E2E Testing - Test User Credentials
E2E_TEST_EMAIL=your-test-user@example.com
E2E_TEST_PASSWORD=your-test-password

# Database Direct Connection (for MCP postgres server)
# Password must be URL-encoded if it contains special characters
SUPABASE_DB_PASSWORD=your-db-password
SUPABASE_DB_HOST=db.your-project.supabase.co
SUPABASE_DB_URL=postgresql://postgres:your-url-encoded-password@db.your-project.supabase.co:5432/postgres
